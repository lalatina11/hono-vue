{
  "name": "hono-vue",
  "type": "module",
  "main": "server/src/index.ts",
  "scripts": {
    "dev:server": "tsx watch server/src/index.ts",
    "dev:client": "cd client && npm run dev",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.3",
    "concurrently": "^9.1.2",
    "hono": "^4.7.11"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  }
}